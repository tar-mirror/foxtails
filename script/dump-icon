#!/usr/bin/env ruby

# NOTE THIS MAKES GIF, NOT PNG -- use gif2png -O

d = DATA.gets(nil).split(/[\n,\s]+/)
d.shift
d.map! {|x| x.hex }
dd = d.pack("C*")

if ARGV[0]
  File.open(ARGV[0], "w") { |f| f.write dd }
else
  puts "Usage: #{$0} iconfile"
end

__END__
